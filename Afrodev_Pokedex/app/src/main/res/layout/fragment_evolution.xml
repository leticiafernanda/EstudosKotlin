<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="593dp"
        android:layout_height="237dp"
        android:background="#FF5722"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:titleTextColor="@color/purple_700"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-54dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="122dp"
        android:layout_height="126dp"
        android:src="@mipmap/charmander"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="348dp"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.chip.Chip
        android:id="@+id/btn_habilidades"
        android:layout_width="120dp"
        android:layout_height="41dp"
        android:layout_gravity="center_horizontal"
        android:text="HABILIDADES"
        android:textAlignment="center"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="272dp"
        tools:layout_editor_absoluteY="237dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/btn_evolucao"
        android:layout_width="123dp"
        android:layout_height="43dp"
        android:backgroundTint="#FF5722"
        android:gravity="center_horizontal"
        android:text="EVOLUÇÃO"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="237dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/btn_status"
        android:layout_width="128dp"
        android:layout_height="45dp"
        android:gravity="center_horizontal"
        android:text="STATUS"
        android:textAlignment="center"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="237dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="197dp"
        android:layout_height="42dp"
        android:fontFamily="casual"
        android:text="Charmander"
        android:textColor="@color/cardview_light_background"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="104dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="119dp"
        android:layout_height="29dp"
        android:fontFamily="@font/circularstd_medium"
        android:text="Charmander"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="474dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="119dp"
        android:layout_height="29dp"
        android:fontFamily="@font/circularstd_medium"
        android:text="Charmeleon"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="269dp"
        tools:layout_editor_absoluteY="474dp" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="131dp"
        android:layout_height="23dp"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="408dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="150dp"
        android:layout_height="165dp"
        app:srcCompat="@mipmap/evolucao"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="528dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="156dp"
        android:layout_height="153dp"
        app:srcCompat="@mipmap/evolucao2"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="528dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="119dp"
        android:layout_height="29dp"
        android:fontFamily="@font/circularstd_medium"
        android:text="Charmeleon"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="681dp" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="134dp"
        android:layout_height="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="608dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="119dp"
        android:layout_height="29dp"
        android:fontFamily="@font/circularstd_medium"
        android:text="Charizard"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="276dp"
        tools:layout_editor_absoluteY="681dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="156dp"
        android:layout_height="153dp"
        app:srcCompat="@mipmap/evolucao2"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="329dp"
        tools:ignore="MissingConstraints" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="155dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </FrameLayout>
    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>